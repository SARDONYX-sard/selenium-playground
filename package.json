{
  "name": "selenium-playground",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/SARDONYX-sard/selenium-playground"
  },
  "main": "src/main.ts",
  "scripts": {
